.rank-list {
  display: flex;
  flex-direction: column;
  text-align: center;
  padding: 0rpx;
}
.rank-title {
  height: 200rpx;
  width: 100%;
  border-bottom: 1px solid darkgray;
}
.rank-title-item{
  padding-top: 40rpx;
  float: left;
  text-align: center;
  width: 50%;
}
.rank-title-number {
  padding-top: 20rpx;
  font-size: 30px;
  color: green;
}
